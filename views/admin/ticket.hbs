            <td>{{this.stud_id}}</td>
            
            <td>{{this.title}}</td>
            <td>{{this.concern}}</td>
            <td>{{this.status}}</td>
            <td>
                
            </td>
            
            
            
             <td>
                <a href="/ticket/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>&nbsp;&nbsp;&nbsp;
               
            </td>
           
            
        </tr>
        {{/each}}
    </tbody>
</table>