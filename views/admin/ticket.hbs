<h3>{{viewTitle2}}</h3><br>


<!-- timetable all data list -->
<table class="table table-striped" id="myTable">
        <thead>
        <tr>
            <th>Id</th>
            <th>Stud_id</th>
            
            <th>Title</th>
            <th>Concern</th>
            <th>Status</th>
            <td>Update</td>
            
            
        </tr>
    </thead>

    <tbody>
